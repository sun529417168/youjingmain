<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/scroll_style"
    android:background="@color/gray_view_bg">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:orientation="vertical">

    <include
        android:id="@+id/title"
        layout="@layout/title_blue_bg" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:orientation="vertical">
            <RelativeLayout
                android:id="@+id/lay_answer_time_order"
                android:layout_width="match_parent"
                android:layout_height="47dp"
                android:background="@drawable/item_change_bg"
                android:clickable="true"
                android:paddingLeft="@dimen/my_common_margin_min"
                android:paddingRight="@dimen/my_common_margin_min">

                <TextView
                    android:id="@+id/txt_answer_time_order"
                    style="@style/middle_black_text_style"
                    android:text="勋章" />

                <ImageView
                    android:id="@+id/btn_answer_time_order"
                    style="@style/right_arrow_style"
                    android:layout_marginLeft="@dimen/my_common_margin_small" />
                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/view_height_width"
                    android:layout_alignParentBottom="true"
                    android:background="@color/view" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/lay_question_note"
                android:layout_width="match_parent"
                android:layout_height="47dp"
                android:background="@drawable/item_change_bg"
                android:clickable="true"
                android:paddingLeft="@dimen/my_common_margin_min"
                android:paddingRight="@dimen/my_common_margin_min">

                <TextView
                    android:id="@+id/txt_question_note"
                    style="@style/middle_black_text_style"
                    android:text="等级" />

                <ImageView
                    android:id="@+id/btn_question_note"
                    style="@style/right_arrow_style" />

            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="9dp"
                android:background="@color/gray_view_bg" />

            <RelativeLayout
                android:id="@+id/re_new_message"
                android:layout_width="match_parent"
                android:layout_height="47dp"
                android:background="@drawable/item_change_bg"
                android:clickable="true"
                android:paddingLeft="@dimen/my_common_margin_min"
                android:paddingRight="@dimen/my_common_margin_min">

                <TextView
                    android:id="@+id/txt_answer_record"
                    style="@style/middle_black_text_style"
                    android:text="新消息提醒" />

                <CheckBox
                    android:id="@+id/cb_new_message"
                    android:layout_width="34dp"
                    android:layout_height="21dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:clickable="false"
                    android:button="@null"
                    android:background="@drawable/checkbox_selecter"/>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/view_height_width"
                    android:layout_alignParentBottom="true"
                    android:background="@color/view" />
            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/re_watch_video"
                android:layout_width="match_parent"
                android:layout_height="47dp"
                android:paddingLeft="@dimen/my_common_margin_min"
                android:background="@drawable/item_change_bg"
                android:clickable="true"
                android:paddingRight="@dimen/my_common_margin_min">

                <TextView
                    android:id="@+id/txt_my_order"
                    style="@style/middle_black_text_style"
                    android:text="3/4G观看视频" />

                <CheckBox
                    android:id="@+id/cb_watch_video"
                    android:layout_width="34dp"
                    android:layout_height="21dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:clickable="false"
                    android:button="@null"
                    android:background="@drawable/checkbox_selecter"/>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/view_height_width"
                    android:layout_alignParentBottom="true"
                    android:background="@color/view" />
            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/re_download_video"
                android:layout_width="match_parent"
                android:layout_height="47dp"
                android:background="@drawable/item_change_bg"
                android:clickable="true"
                android:paddingLeft="@dimen/my_common_margin_min"
                android:paddingRight="@dimen/my_common_margin_min">

                <TextView
                    android:id="@+id/txt_my_card"
                    style="@style/middle_black_text_style"

                    android:layout_toRightOf="@id/img_my_card"
                    android:text="3/4G缓存视频" />

                <CheckBox
                    android:id="@+id/cb_download_video"
                    android:layout_width="34dp"
                    android:layout_height="21dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:clickable="false"
                    android:button="@null"
                    android:background="@drawable/checkbox_selecter"/>

            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="9dp"
                android:background="@color/gray_view_bg" />

            <RelativeLayout
                android:id="@+id/re_wipe_cache"
                android:layout_width="match_parent"
                android:layout_height="47dp"
                android:background="@drawable/item_change_bg"
                android:clickable="true"
                android:paddingLeft="@dimen/my_common_margin_min"
                android:paddingRight="@dimen/my_common_margin_min">

                <TextView
                    style="@style/middle_black_text_style"
                    android:text="清除缓存" />

                <ImageView
                    android:id="@+id/img_active_card_arrow"
                    style="@style/right_arrow_style" />
                <TextView
                    android:id="@+id/txt_wipe_cache"
                    style="@style/middle_black_text_style"
                    android:layout_toLeftOf="@id/img_active_card_arrow"
                    android:text="0.1M"
                    android:layout_marginRight="8dp"
                    android:textColor="@color/gray_text" />
                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/view_height_width"
                    android:layout_alignParentBottom="true"
                    android:background="@color/view" />
            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/rl_send_score"
                android:layout_width="match_parent"
                android:layout_height="47dp"
                android:paddingLeft="@dimen/my_common_margin_min"
                android:background="@drawable/item_change_bg"
                android:clickable="true"
                android:paddingRight="@dimen/my_common_margin_min">


                <TextView
                    android:id="@+id/txt_my_task"
                    style="@style/middle_black_text_style"
                    android:text="给我们评分吧" />

                <ImageView
                    android:id="@+id/img_my_task_arrow"
                    style="@style/right_arrow_style" />
                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/view_height_width"
                    android:layout_alignParentBottom="true"
                    android:background="@color/view" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/re_fed_back"
                android:layout_width="match_parent"
                android:layout_height="47dp"
                android:paddingLeft="@dimen/my_common_margin_min"
                android:background="@drawable/item_change_bg"
                android:clickable="true"
                android:paddingRight="@dimen/my_common_margin_min">


                <TextView
                    android:id="@+id/txt_setting"
                    style="@style/middle_black_text_style"
                    android:layout_toRightOf="@id/img_setting"
                    android:text="帮助与反馈" />

                <ImageView
                    android:id="@+id/img_setting_arrow"
                    style="@style/right_arrow_style" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/view_height_width"
                    android:layout_alignParentBottom="true"
                    android:background="@color/view" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/re_about_app"
                android:layout_width="match_parent"
                android:layout_height="47dp"
                android:clickable="true"
                android:background="@drawable/item_change_bg"
                android:paddingLeft="@dimen/my_common_margin_min"
                android:paddingRight="@dimen/my_common_margin_min">


                <TextView
                    android:id="@+id/txt_about_app"
                    style="@style/middle_black_text_style"
                    android:layout_toRightOf="@id/img_setting"
                    android:text="关于游鲸" />

                <ImageView
                    android:id="@+id/img_about_app"
                    style="@style/right_arrow_style" />


            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="9dp"
                android:background="@color/gray_view_bg" />
            <RelativeLayout
                android:id="@+id/re_login_out"
                android:layout_width="match_parent"
                android:layout_height="47dp"
                android:clickable="true"
                android:background="@drawable/item_change_bg"
                android:paddingLeft="@dimen/my_common_margin_min"
                android:paddingRight="@dimen/my_common_margin_min">

                <TextView
                    style="@style/middle_black_text_style"
                    android:layout_centerInParent="true"
                    android:textColor="@color/blue_title"
                    android:layout_toRightOf="@id/img_setting"
                    android:text="退出登录" />

            </RelativeLayout>
        </LinearLayout>
    </LinearLayout>
</ScrollView>
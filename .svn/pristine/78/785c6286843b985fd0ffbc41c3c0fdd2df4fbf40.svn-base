public class YJRecommendBook {

    private String microShopAddress;//微店地址
    private String name;//图书名称
    private String bookId;//图书ID
    private String bookPic; //图书图片

    public String getMicroShopAddress() {
        return microShopAddress;
    }

    public void setMicroShopAddress(String microShopAddress) {
        this.microShopAddress = microShopAddress;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getBookId() {
        return bookId;
    }

    public void setBookId(String bookId) {
        this.bookId = bookId;
    }

    public String getBookPic() {
        return bookPic;
    }

    public void setBookPic(String bookPic) {
        this.bookPic = bookPic;
    }

    @Override
    public String toString() {
        return "YJRecommendBook{" +
                "microShopAddress='" + microShopAddress + '\'' +
                ", name='" + name + '\'' +
                ", bookId='" + bookId + '\'' +
                ", bookPic='" + bookPic + '\'' +
                '}';
    }
}

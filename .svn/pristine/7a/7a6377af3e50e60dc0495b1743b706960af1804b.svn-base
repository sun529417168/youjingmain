public class StringWhiteSpaceUtil {
    public static boolean checkIsWhiteSpace(char c)
    {
        boolean isWhiteSpace = false;
        if (Character.isWhitespace(c))
        {
            isWhiteSpace = true;
        }
        return isWhiteSpace;
    }
}

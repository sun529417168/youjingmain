<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/relayout_my_cards"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">


    <include
        android:id="@+id/title"
        layout="@layout/title_blue_bg" />

    <RelativeLayout
        android:id="@+id/re_order_num"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/img_message_point"
            android:layout_width="3dp"
            android:layout_height="17dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="13dp"
            android:layout_centerVertical="true"
            android:background="@color/yellow_text" />

        <TextView
            android:id="@+id/txt_message_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginBottom="3dp"
            android:layout_marginLeft="7dp"
            android:singleLine="true"
            android:maxEms="18"

            android:ellipsize="end"
            android:layout_toRightOf="@id/img_message_point"
            android:text="什么是优惠券？"
            android:textColor="@color/black"
            android:textSize="16sp" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:layout_below="@id/re_order_num"
        android:layout_marginBottom="8dp"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/txt_message_content"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:lineSpacingExtra="3dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="13dp"
            android:layout_marginRight="13dp"
            android:text="优惠券是游鲸课堂发行和认可的购课低价券，可以在游鲸课堂手机客户端和官网上购买课程时低价相应的金额"
            android:textColor="@color/black"
            android:textSize="14sp" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/view_height_width"
        android:layout_marginLeft="9dp"
        android:layout_marginRight="9dp"
        android:background="@color/view" />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/img_message_point_t"
            android:layout_width="3dp"
            android:layout_height="17dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="13dp"
            android:layout_centerVertical="true"
            android:background="@color/yellow_text" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginBottom="3dp"
            android:layout_marginLeft="7dp"
            android:singleLine="true"
            android:maxEms="18"
            android:ellipsize="end"
            android:layout_toRightOf="@id/img_message_point_t"
            android:text="如何获得优惠券？"
            android:textColor="@color/black"
            android:textSize="16sp" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:layout_below="@id/re_order_num"
        android:layout_marginBottom="8dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:lineSpacingExtra="3dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="13dp"
            android:layout_marginRight="13dp"
            android:text="游鲸课堂手机客户端和官网会不定期推出活动，手机用户可以打开游鲸课堂手机客户端，关注新上的活动，机会不容错过！"
            android:textColor="@color/black"
            android:textSize="14sp" />
    </RelativeLayout>

    <View
    android:layout_width="match_parent"
    android:layout_height="@dimen/view_height_width"
    android:layout_marginLeft="9dp"
    android:layout_marginRight="9dp"
    android:background="@color/view" />


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/img_message_point_f"
            android:layout_width="3dp"
            android:layout_height="17dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="13dp"
            android:layout_centerVertical="true"
            android:background="@color/yellow_text" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginBottom="3dp"
            android:layout_marginLeft="7dp"
            android:singleLine="true"
            android:maxEms="18"
            android:ellipsize="end"
            android:layout_toRightOf="@id/img_message_point_f"
            android:text="如何使用优惠券？"
            android:textColor="@color/black"
            android:textSize="16sp" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:layout_below="@id/re_order_num"
        android:layout_marginBottom="8dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:lineSpacingExtra="3dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="13dp"
            android:layout_marginRight="13dp"
            android:text="在官网和手机客户端付款时，可以选择使用优惠券页面中未使用且未过期的优惠券，选择后支付订单，课低价相应的金额"
            android:textColor="@color/black"
            android:textSize="14sp" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/view_height_width"
        android:layout_marginLeft="9dp"
        android:layout_marginRight="9dp"
        android:background="@color/view" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/img_message_point_s"
            android:layout_width="3dp"
            android:layout_height="17dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="13dp"
            android:layout_centerVertical="true"
            android:background="@color/yellow_text" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginBottom="3dp"
            android:layout_marginLeft="7dp"
            android:singleLine="true"
            android:maxEms="18"
            android:ellipsize="end"
            android:layout_toRightOf="@id/img_message_point_s"
            android:text="优惠券找零兑现吗？"
            android:textColor="@color/black"
            android:textSize="16sp" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:layout_below="@id/re_order_num"
        android:layout_marginBottom="8dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:lineSpacingExtra="3dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="13dp"
            android:layout_marginRight="13dp"
            android:text="优惠券不找领，不兑现。付款时低价金额会优先使用，不足支付时才会使用现金支付"
            android:textColor="@color/black"
            android:textSize="14sp" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/view_height_width"
        android:layout_marginLeft="9dp"
        android:layout_marginRight="9dp"
        android:background="@color/view" />
</LinearLayout>